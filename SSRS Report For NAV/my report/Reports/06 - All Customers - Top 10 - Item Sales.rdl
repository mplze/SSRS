    <DataSet Name="StartOfFinancialYear">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     
	top 1 [Starting Date] as FromDate
FROM         
	[CRONUS International Ltd_$Accounting Period]
WHERE     
	([New Fiscal Year] = 1)
	AND
	[Starting Date]&lt;=(		SELECT     TOP 1 
					[Posting Date]
				FROM         
					[CRONUS International Ltd_$Value Entry]
				ORDER BY 
					[Posting Date] Desc
			)

order by
	[Starting Date] desc</CommandText>
        <DataSourceName>NavisionRS</DataSourceName>
      </Query>
      <Fields>
        <Field Name="FromDate">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>FromDate</DataField>
        </Field>
      </Fields>
    </DataSet>
