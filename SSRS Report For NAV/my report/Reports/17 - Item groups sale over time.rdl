  <DataSets>
    <DataSet Name="SalesOverTime">
      <Query>
        <CommandText>SELECT     DATEPART(Year, [CRONUS International Ltd_$Value Entry].[Posting Date]) AS YEAR, [CRONUS International Ltd_$Item].[Inventory Posting Group], 
                      DATEPART(Quarter, [CRONUS International Ltd_$Value Entry].[Posting Date]) AS QUARTER, DATENAME(Month, 
                      [CRONUS International Ltd_$Value Entry].[Posting Date]) AS MONTHNAME, SUM([CRONUS International Ltd_$Value Entry].[Sales Amount (Actual)]) 
                      AS [Sales Amount(Actual)], SUM([CRONUS International Ltd_$Value Entry].[Cost Amount (Actual)]) AS [Cost Amount(Actual)], 
                      SUM([CRONUS International Ltd_$Value Entry].Adjustment) AS Adjustment, 
                      SUM([CRONUS International Ltd_$Value Entry].[Sales Amount (Actual)] + [CRONUS International Ltd_$Value Entry].[Cost Amount (Actual)] + [CRONUS International Ltd_$Value Entry].Adjustment)
                       AS Profit, DATEPART(Month, [CRONUS International Ltd_$Value Entry].[Posting Date]) AS MonthNo, [CRONUS International Ltd_$Value Entry].[Item No_], 
                      SUM(- [CRONUS International Ltd_$Value Entry].[Invoiced Quantity]) AS Quantity
FROM         [CRONUS International Ltd_$Value Entry] LEFT OUTER JOIN
                      [CRONUS International Ltd_$Item] ON [CRONUS International Ltd_$Value Entry].[Item No_] = [CRONUS International Ltd_$Item].No_
WHERE     ([CRONUS International Ltd_$Value Entry].[Item Ledger Entry Type] = 1) AND ([CRONUS International Ltd_$Value Entry].[Expected Cost] &lt;&gt; 1) AND 
                      ([CRONUS International Ltd_$Value Entry].[Invoiced Quantity] &lt;&gt; 0)
GROUP BY DATEPART(Year, [CRONUS International Ltd_$Value Entry].[Posting Date]), DATEPART(Quarter, [CRONUS International Ltd_$Value Entry].[Posting Date]), 
                      DATENAME(Month, [CRONUS International Ltd_$Value Entry].[Posting Date]), DATEPART(Month, [CRONUS International Ltd_$Value Entry].[Posting Date]), 
                      [CRONUS International Ltd_$Value Entry].[Item No_], [CRONUS International Ltd_$Item].[Inventory Posting Group]
HAVING      (SUM([CRONUS International Ltd_$Value Entry].[Sales Amount (Actual)]) &lt;&gt; 0) AND ([CRONUS International Ltd_$Item].[Inventory Posting Group] = @InvPostGrp)
ORDER BY DATEPART(Month, [CRONUS International Ltd_$Value Entry].[Posting Date]), DATEPART(Year, [CRONUS International Ltd_$Value Entry].[Posting Date])</CommandText>
        <QueryParameters>
          <QueryParameter Name="@InvPostGrp">
            <Value>=Parameters!InvPostGrp.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>NavisionRS</DataSourceName>
      </Query>
      <Fields>
        <Field Name="YEAR">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>YEAR</DataField>
        </Field>
        <Field Name="Inventory_Posting_Group">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Inventory Posting Group</DataField>
        </Field>
        <Field Name="QUARTER">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>QUARTER</DataField>
        </Field>
        <Field Name="MONTHNAME">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>MONTHNAME</DataField>
        </Field>
        <Field Name="Sales_Amount_Actual_">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Sales Amount(Actual)</DataField>
        </Field>
        <Field Name="Cost_Amount_Actual_">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Cost Amount(Actual)</DataField>
        </Field>
        <Field Name="Adjustment">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Adjustment</DataField>
        </Field>
        <Field Name="Profit">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Profit</DataField>
        </Field>
        <Field Name="MonthNo">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>MonthNo</DataField>
        </Field>
        <Field Name="Item_No_">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Item No_</DataField>
        </Field>
        <Field Name="Quantity">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Quantity</DataField>
        </Field>
      </Fields>
    </DataSet>
