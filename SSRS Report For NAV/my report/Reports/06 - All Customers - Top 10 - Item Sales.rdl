  <DataSets>
    <DataSet Name="LastYear">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT    
	[CRONUS International Ltd_$Customer].No_, 
	[CRONUS International Ltd_$Customer].Name AS Customer, 
              SUM([CRONUS International Ltd_$Value Entry].[Sales Amount (Actual)]) AS SalesAmount
	
FROM         
	[CRONUS International Ltd_$Customer] 
	INNER JOIN
              [CRONUS International Ltd_$Value Entry] 
	ON 
	[CRONUS International Ltd_$Customer].No_ = [CRONUS International Ltd_$Value Entry].[Source No_]
WHERE     
	([CRONUS International Ltd_$Value Entry].[Expected Cost] &lt;&gt; 1) 
	AND 
	([CRONUS International Ltd_$Value Entry].[Item Ledger Entry Type] = 1) 
	AND 
              ([CRONUS International Ltd_$Value Entry].[Invoiced Quantity] &lt;&gt; 0) 
	AND 
	 [CRONUS International Ltd_$Value Entry].[Posting Date] &lt;= DATEADD(year,-1,CONVERT(datetime, @To, 103))
	AND
	DATEADD(year,-1,CONVERT(datetime, @From, 103))  &lt;= [CRONUS International Ltd_$Value Entry].[Posting Date] 
GROUP BY 
	[CRONUS International Ltd_$Customer].No_, 
	[CRONUS International Ltd_$Customer].Name
ORDER BY 
	SUM([CRONUS International Ltd_$Value Entry].[Sales Amount (Actual)]) DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@To">
            <Value>=Parameters!To.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@From">
            <Value>=Parameters!From.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>NavisionRS</DataSourceName>
      </Query>
      <Fields>
        <Field Name="No_">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>No_</DataField>
        </Field>
        <Field Name="Customer">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Customer</DataField>
        </Field>
        <Field Name="SalesAmount">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>SalesAmount</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CurrentYear">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     
	[CRONUS International Ltd_$Customer].No_, 
	[CRONUS International Ltd_$Customer].Name AS Customer, 
              SUM([CRONUS International Ltd_$Value Entry].[Sales Amount (Actual)]) AS SalesAmount
	
FROM         
	[CRONUS International Ltd_$Customer] 
	INNER JOIN
              [CRONUS International Ltd_$Value Entry] 
	ON 
	[CRONUS International Ltd_$Customer].No_ = [CRONUS International Ltd_$Value Entry].[Source No_]
WHERE     
	([CRONUS International Ltd_$Value Entry].[Expected Cost] &lt;&gt; 1) 
	AND 
	([CRONUS International Ltd_$Value Entry].[Item Ledger Entry Type] = 1) 
	AND 
              ([CRONUS International Ltd_$Value Entry].[Invoiced Quantity] &lt;&gt; 0) 
	AND 
	 [CRONUS International Ltd_$Value Entry].[Posting Date] &lt;= CONVERT(datetime, @To, 103) 
	AND
	( CONVERT(datetime, @From, 103)  &lt;= [CRONUS International Ltd_$Value Entry].[Posting Date] )
GROUP BY 
	[CRONUS International Ltd_$Customer].No_, 
	[CRONUS International Ltd_$Customer].Name
ORDER BY 
	SUM([CRONUS International Ltd_$Value Entry].[Sales Amount (Actual)]) DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@To">
            <Value>=Parameters!To.Value</Value>
