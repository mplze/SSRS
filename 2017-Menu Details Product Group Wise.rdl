        <Field Name="PCode">
          <DataField>PCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description_">
          <DataField>Description_</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Inventory">
