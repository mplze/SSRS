  <DataSets>
    <DataSet Name="Sales">
      <Query>
        <CommandText>SELECT     [CRONUS International Ltd_$Customer].No_ + ' ' + [CRONUS International Ltd_$Customer].Name AS Customer, 
                      [CRONUS International Ltd_$Value Entry].[Document No_] AS DocumentNo, [CRONUS International Ltd_$Value Entry].[Posting Date], DATEPART(Year, 
                      [CRONUS International Ltd_$Value Entry].[Posting Date]) AS YEAR, DATEPART(Quarter, [CRONUS International Ltd_$Value Entry].[Posting Date]) AS QUARTER, 
                      DATENAME(Month, [CRONUS International Ltd_$Value Entry].[Posting Date]) AS MONTHNAME, DATEPART(Month, 
                      [CRONUS International Ltd_$Value Entry].[Posting Date]) AS MONTH, SUM([CRONUS International Ltd_$Value Entry].[Sales Amount (Actual)]) 
                      AS [Sales Amount(Actual)], SUM([CRONUS International Ltd_$Value Entry].[Cost Amount (Actual)]) AS [Cost Amount(Actual)], 
                      SUM([CRONUS International Ltd_$Value Entry].Adjustment) AS Adjustment, [CRONUS International Ltd_$Customer].Address, 
                      SUM([CRONUS International Ltd_$Value Entry].[Sales Amount (Actual)] + [CRONUS International Ltd_$Value Entry].[Cost Amount (Actual)] + [CRONUS International Ltd_$Value Entry].Adjustment)
                       AS Profit, [CRONUS International Ltd_$Customer].City, [CRONUS International Ltd_$Customer].[Language Code]
FROM         [CRONUS International Ltd_$Value Entry] INNER JOIN
                      [CRONUS International Ltd_$Customer] ON [CRONUS International Ltd_$Value Entry].[Source No_] = [CRONUS International Ltd_$Customer].No_
WHERE     ([CRONUS International Ltd_$Value Entry].[Item Ledger Entry Type] = 1) AND ([CRONUS International Ltd_$Value Entry].[Expected Cost] &lt;&gt; 1) AND 
                      ([CRONUS International Ltd_$Value Entry].[Invoiced Quantity] &lt;&gt; 0) AND ([CRONUS International Ltd_$Customer].No_ = @customer)
GROUP BY [CRONUS International Ltd_$Customer].No_ + ' ' + [CRONUS International Ltd_$Customer].Name, [CRONUS International Ltd_$Value Entry].[Document No_], 
                      [CRONUS International Ltd_$Value Entry].[Posting Date], DATEPART(Year, [CRONUS International Ltd_$Value Entry].[Posting Date]), DATEPART(Quarter, 
                      [CRONUS International Ltd_$Value Entry].[Posting Date]), DATENAME(Month, [CRONUS International Ltd_$Value Entry].[Posting Date]), 
                      [CRONUS International Ltd_$Customer].Address, [CRONUS International Ltd_$Customer].City, [CRONUS International Ltd_$Customer].[Language Code]
ORDER BY DATEPART(Year, [CRONUS International Ltd_$Value Entry].[Posting Date]) DESC, DATEPART(Month, [CRONUS International Ltd_$Value Entry].[Posting Date])</CommandText>
        <QueryParameters>
          <QueryParameter Name="@customer">
            <Value>=Parameters!customer.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>NavisionRS</DataSourceName>
      </Query>
