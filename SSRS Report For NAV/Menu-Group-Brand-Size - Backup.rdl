  <DataSets>
    <DataSet Name="Menu">
      <Query>
        <DataSourceName>DB1</DataSourceName>
        <CommandText>Declare @Stock table (
			Location varchar(20),
			PCode varchar(20),
			Brand varchar (20),
		    Size  varchar (20), 
		    Qty decimal
)


Insert into @Stock
SELECT   V.[Location Code],
         V.[Product Group Code], 
		 V.[Brand Code],
		 I.[Variant Dimension 2],
		 Sum(V.Quantity) as QTY	 
	 
FROM    [Hameedia Stores Pvt Ltd$Item Ledger Entry] V join 
        [Hameedia Stores Pvt Ltd$Item Variant Registration] I on (I.[Item No_]=V.[Item No_] and I.Variant=V.[Variant Code] )
       
WHERE   V.[Location Code]='HDT'
Group by V.[Location Code], V.[Product Group Code], V.[Brand Code],I.[Variant Dimension 2]
Having Sum(V.Quantity)&lt;&gt;0

--Show


Select M.[Product Group Code],M.Brand,M.Size,Sum(M.Quantity) as 'Menu Qty',sum(S.Qty) as Inventory ,Sum(M.Quantity)-sum(S.Qty) as Variance
From [Hameedia Stores Pvt Ltd$Retail Sales Budget Entry] M Join @Stock S on
     (M.[Store No_]=S.Location and M.[Product Group Code]=S.PCode and M.Size=S.Size) 
Where [Budget Name]='MENU'
Group by M.[Product Group Code],M.Brand,M.Size</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
