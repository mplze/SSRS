      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>Begin 
DECLARE @info table (
	Com varchar(10),
	Loc varchar(20),
	ProductG varchar(10),
	Brand varchar(10),
	Qty float
)

DECLARE @info2 table (
	Com varchar(10),
	Loc varchar(20),
	ProductG varchar(10),
	Brand varchar(10),
	Qty float
)
-- ======================HS======================
insert into @info (Loc,ProductG,Brand,Qty)
SELECT [Location Code],[Product Group Code],[Brand Code],Sum(Quantity) as Qty
FROM [dbo].[Hameedia Stores Pvt Ltd$Item Ledger Entry]
Group by [Location Code],[Product Group Code],[Brand Code]
Having Sum(Quantity)&lt;&gt;0

Update @info
SET Com='HS'

insert into @info2 
Select * from @info

Delete from @info


--=====================HSC============================================
insert into @info (Loc,ProductG,Brand,Qty)
SELECT [Location Code],[Product Group Code],[Brand Code],Sum(Quantity) as Qty
FROM [HS Clothing Private Limited$Item Ledger Entry]
Group by [Location Code],[Product Group Code],[Brand Code]
Having Sum(Quantity)&lt;&gt;0

Update @info
SET Com='HSC'

insert into @info2 
Select * from @info

Delete from @info

--=====================HSM============================================

insert into @info (Loc,ProductG,Brand,Qty)
SELECT [Location Code],[Product Group Code],[Brand Code],Sum(Quantity) as Qty
FROM [HS Marketing Private Limited$Item Ledger Entry]
Group by [Location Code],[Product Group Code],[Brand Code]
Having Sum(Quantity)&lt;&gt;0

Update @info
SET Com='HSM'

insert into @info2 
Select * from @info

Delete from @info



--======================Perks===============
insert into @info (Loc,ProductG,Brand,Qty)
SELECT [Location Code],[Product Group Code],[Brand Code],Sum(Quantity) as Qty
FROM [dbo].[PERKS Clothing Private Limited$Item Ledger Entry]
Group by [Location Code],[Product Group Code],[Brand Code]
Having Sum(Quantity)&lt;&gt;0

Update @info
SET Com='Perks'

insert into @info2 
Select * from @info

Delete from @info




Select * from @info2
END</CommandText>
