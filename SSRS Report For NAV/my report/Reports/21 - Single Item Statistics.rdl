  <DataSets>
    <DataSet Name="items">
      <Query>
        <CommandText>SELECT     No_, Description, [Unit Price], [Standard Cost], No_ + ' - ' + Description AS ItemName
FROM         [CRONUS International Ltd_$Item]</CommandText>
        <DataSourceName>NavisionRS</DataSourceName>
      </Query>
      <Fields>
        <Field Name="No_">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>No_</DataField>
        </Field>
        <Field Name="Description">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Description</DataField>
        </Field>
        <Field Name="Unit_Price">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Unit Price</DataField>
        </Field>
        <Field Name="Standard_Cost">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Standard Cost</DataField>
        </Field>
        <Field Name="ItemName">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>ItemName</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ItemSales_ProfitPerMonth">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     
	DATEPART(YEAR, [Posting Date]) AS YEAR, 
	DATEPART(Quarter, [Posting Date]) AS Quarter, 
	DATEPART(Month, [Posting Date])  AS MonthNo, 
	DATENAME(Month, [Posting Date]) AS MonthName, 
	- Sum([Invoiced Quantity]) AS Quantity, 
	SUM([Sales Amount (Actual)]) AS Sales, 
               SUM([Sales Amount (Actual)] + [Cost Amount (Actual)] + Adjustment) AS Profit
FROM         
	[CRONUS International Ltd_$Value Entry]
WHERE     
	([Expected Cost] &lt;&gt; 1) 
	AND 
	([Item Ledger Entry Type] = 1) 
	AND 
	([Item No_] = @itemid)
GROUP BY 
	DATEPART(YEAR, [Posting Date]), 
	DATEPART(Quarter, [Posting Date]), 
	DATEPART(Month, [Posting Date]), 
              DATENAME(Month, [Posting Date])
HAVING      
	(SUM([Invoiced Quantity]) &lt;&gt; 0)
ORDER BY 
	DATEPART(Quarter, [Posting Date]), 
	DATEPART(YEAR, [Posting Date])</CommandText>
        <QueryParameters>
          <QueryParameter Name="@itemid">
            <Value>=Parameters!itemid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>NavisionRS</DataSourceName>
      </Query>
      <Fields>
        <Field Name="YEAR">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>YEAR</DataField>
        </Field>
        <Field Name="Quarter">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Quarter</DataField>
        </Field>
        <Field Name="MonthNo">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>MonthNo</DataField>
        </Field>
        <Field Name="MonthName">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>MonthName</DataField>
        </Field>
        <Field Name="Quantity">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Quantity</DataField>
        </Field>
        <Field Name="Sales">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Sales</DataField>
        </Field>
        <Field Name="Profit">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Profit</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ComparedTotalSales">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>Select 
	Totals.SalesAmount AS SalesTotal, 
	Totals.Profit AS ProfitTotal,
	Totals.Year AS YearTotal,
	ChosenOne.SalesAmount,
	ChosenOne.Profit,
	(ChosenOne.SalesAmount/Totals.SalesAmount) AS ShareOfSalesTotal,
	(ChosenOne.Profit/Totals.Profit) AS ShareOfProfitTotal
	
From
(
	SELECT     
		SUM([Sales Amount (Actual)]) AS SalesAmount, 
		SUM([Cost Amount (Actual)]) AS CostAmount, 
		SUM(Adjustment) AS Adjustment, 
                      	SUM([Sales Amount (Actual)] + [Cost Amount (Actual)] + Adjustment) AS Profit, 
		'Andre' AS ItemNo, 
		DATEPART(Year, [Posting Date]) AS Year
	FROM         
		[CRONUS International Ltd_$Value Entry]
	WHERE     
		([Item Ledger Entry Type] = 1) 
		AND 
		([Expected Cost] &lt;&gt; 1) 
		AND 
		([Invoiced Quantity] &lt;&gt; 0) 
	GROUP BY 
		DATEPART(Year, [Posting Date])
)Totals

LEFT OUTER JOIN

(
	SELECT 
		SUM([Sales Amount (Actual)]) AS SalesAmount, 
		SUM([Cost Amount (Actual)]) AS CostAmount, 
		SUM(Adjustment) AS Adjustment, 
                      	SUM([Sales Amount (Actual)] + [Cost Amount (Actual)] + Adjustment) AS Profit, 
		[Item No_], 
		DATEPART(Year, [Posting Date]) AS Year
	FROM
		[CRONUS International Ltd_$Value Entry]
	WHERE     
		([Item Ledger Entry Type] = 1) 
		AND ([Expected Cost] &lt;&gt; 1) 
		AND ([Invoiced Quantity] &lt;&gt; 0) 
		AND ([Item No_] = @itemid)

	GROUP BY 
		[Item No_], 
		DATEPART(Year, [Posting Date])
)ChosenOne
ON Totals.Year = ChosenOne.Year</CommandText>
        <QueryParameters>
          <QueryParameter Name="@itemid">
            <Value>=Parameters!itemid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>NavisionRS</DataSourceName>
      </Query>
      <Fields>
        <Field Name="SalesTotal">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>SalesTotal</DataField>
        </Field>
        <Field Name="ProfitTotal">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>ProfitTotal</DataField>
        </Field>
        <Field Name="YearTotal">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>YearTotal</DataField>
        </Field>
        <Field Name="SalesAmount">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>SalesAmount</DataField>
        </Field>
        <Field Name="Profit">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Profit</DataField>
        </Field>
        <Field Name="ShareOfSalesTotal">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>ShareOfSalesTotal</DataField>
        </Field>
        <Field Name="ShareOfProfitTotal">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>ShareOfProfitTotal</DataField>
        </Field>
      </Fields>
    </DataSet>
