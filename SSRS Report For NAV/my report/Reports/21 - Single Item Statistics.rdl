    <DataSet Name="LatestSalesDate">
      <Query>
        <CommandText>SELECT     TOP 1 [Posting Date], - ([Sales Amount (Actual)] / [Invoiced Quantity]) AS LatestSalesPricePerUnit
FROM         [CRONUS International Ltd_$Value Entry]
